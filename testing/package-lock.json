{
  "name": "root",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "root",
      "workspaces": [
        "packages/*"
      ],
      "dependencies": {
        "@root/hello": "file:packages/hello",
        "@root/meddile": "file:packages/meddile"
      }
    },
    "node_modules/@root/hello": {
      "resolved": "packages/hello",
      "link": true
    },
    "node_modules/@root/meddile": {
      "resolved": "packages/meddile",
      "link": true
    },
    "packages/hello": {
      "name": "@root/hello"
    },
    "packages/meddile": {}
  },
  "dependencies": {
    "@root/hello": {
      "version": "file:packages/hello"
    },
    "@root/meddile": {
      "version": "file:packages/meddile"
    }
  }
}
